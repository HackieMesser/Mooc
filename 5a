#01
def longest(strings: list):
    string02 = strings[:]
    string02.sort(key=len)
    return (string02[-1])
    
#02
def count_matching_elements(my_matrix: list, element: int):
    count=0
    for i in my_matrix:
        for x in i:
            if x == element:
                count+=1
    return count
#03
def who_won(game_board: list):
    count0=0
    count1=0
    count2=0
    for rows in game_board:
        for square in rows:
            if square == 0:
                count0+=1
            elif square ==1:
                count1+=1
            elif square ==2:
                count2+=1

    if count2> count1:
        return 2
    elif count1>count2:
        return 1
    else:
        return 0
