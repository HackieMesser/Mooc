#01
def longest(strings: list):
    string02 = strings[:]
    string02.sort(key=len)
    return (string02[-1])
    
#02
def count_matching_elements(my_matrix: list, element: int):
    count=0
    for i in my_matrix:
        for x in i:
            if x == element:
                count+=1
    return count
#03
def who_won(game_board: list):
    count0=0
    count1=0
    count2=0
    for rows in game_board:
        for square in rows:
            if square == 0:
                count0+=1
            elif square ==1:
                count1+=1
            elif square ==2:
                count2+=1

    if count2> count1:
        return 2
    elif count1>count2:
        return 1
    else:
        return 0
#04
def row_correct(sudoku: list, row_no:int):
    columns=sudoku[row_no]
    columns.sort()
    print(columns)
    i2=0
    for i in columns:
        if i2!=0 and i2 ==i:
            return False
        i2=i
    return True
#04 anotherway    
def row_correct(sudoku: list, row_no:int):
    numbers=[]
    for i in sudoku[row_no]:
        if i!=0 and i in numbers:
            return False
        numbers.append(i)
    return True
#05
def column_correct(sudoku: list, column_no:int):
    numbers=[]
    for i in range(0,(len(sudoku)-1)):
        if (sudoku[i][column_no])!=0 and (sudoku[i][column_no]) in numbers:
            return False
        numbers.append(sudoku[i][column_no])
    return True
#06 work in progress
def block_correct(sudoku:list, row_no:int, column_no:int):
    numbers=[]
    for i in range(0,2):
        if sudoku[row_no][column_no+i] !=0 and sudoku[row_no][column_no+i] in numbers:
            return False
        numbers.append(sudoku[row_no][column_no+i])
    row_no+=1
    for i in range(0,2):
        if sudoku[row_no][column_no+i] !=0 and sudoku[row_no][column_no+i] in numbers:
            return False
        numbers.append(sudoku[row_no][column_no+i])
    row_no+=1
    for i in range(0,2):
        if sudoku[row_no][column_no+i] !=0 and sudoku[row_no][column_no+i] in numbers:
            return False
        numbers.append(sudoku[row_no][column_no+i])
    return True

#07
def block_correct(sudoku:list, row_no:int, column_no:int):
    numbers=[]
    for i in range(0,3):    
        for i1 in range(0,3):
            if sudoku[(row_no)+i1][(column_no)+i] !=0 and sudoku[(row_no)+i1][(column_no)+i] in numbers:
                return False
            numbers.append(sudoku[(row_no)+i1][(column_no)+i])
    return True
#another way
def block_correct(sudoku:list, row_no:int, column_no:int):
    numbers=[]
    for i in range(0,3):    
        for i1 in range(0,3):
            number = sudoku[row_no+i][column_no+i1]
            if number !=0 and number in numbers:
                return False
            numbers.append(sudoku[row_no+i][column_no+i1])
    return True









