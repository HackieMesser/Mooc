#everything reveresed

def everything_reversed(a):
    c=[]
    
    if len(a)>1:
        for i in a[::-1]:

            c.append(i[::-1])


    return c

#most_common_character
def most_common_character (a: str):
    new=""
    number=0
    new_number=0
    for i in a:
        number=a.count(i)
        if new_number<number:
            new_number=number
            new=i 
    return new
